<h1>Diaper Change</h1>

<div class='center'>
  <% @diaper_changes.each do |d| %>
    <fieldset>
      <%= d.created_at.strftime("%a %B %d, %Y") %><br>
      <%= d.change_time %><br>
      <%= d.diaper_type %>
    </fieldset>
    <br><br>
  <% end %>

  <%= render partial: 'form' %>

  <br><br>
  <%= link_to 'Back', user_index_path %>
</div>
